@{
}
@model library.Models.AddStudentViewModel

<h1>Add Student </h1>
<form method="post"  >

    <div>
        <div class="mt-3">
            <label class="form-label">Name</label>
            <input type="text" class="form-control" asp-for="Name" />
        </div>

        <div class="mt-3">
            <label class="form-label">Roll</label>
            <input type="text" class="form-control" asp-for="StudentRoll" />
        </div>

        <div class="mt-3">
            <label class="form-label">Gender</label>
            <input type="text" class="form-control" asp-for="Gender" />
        </div>

        <div class="mt-3">
            <label class="form-label">Std/Class</label>
            <input type="text" class="form-control" asp-for="Std" />
        </div>

        <div class="mt-3">
            <button type="submit" class="btn btn-primary">Submit</button>
        </div>


    </div>


</form>


