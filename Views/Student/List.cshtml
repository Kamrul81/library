@{
}

@model List<library.Models.Student>

<h1>Student List</h1>
<table class="table">

    <thead>
        <tr>
            <th>Name</th>
            <th>Roll</th>
            <th>Gender</th>
            <th>Std/Class</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var student in Model)
        {
            <tr>
                <td> @student.Name </td>
                <td> @student.StudentRoll</td>
                <td> @student.Gender</td>
                <td> @student.Std</td>
                <td>
                    <a class="btn btn-primary" asp-controller="Student" asp-action="Edit" asp-route-id="@student.ID">Edit</a>
                </td>
            </tr>
        }
    </tbody>

</table>